{
	"compilerOptions": {
		"target": "es2020",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
	},
	"exclude": [
		"node_modules"
	]
}